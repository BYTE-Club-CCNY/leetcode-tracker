{
    "manifest_version": 3,
    "name": "TrackLeet",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icon32.png",
        "32": "icon32.png",
        "48": "icon32.png",
        "128":"icon32.png"
    }
  }